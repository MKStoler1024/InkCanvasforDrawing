                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Opacity="{Binding ElementName=TwoFingerGestureSimpleStackPanel, Path=Opacity}"
                                    Orientation="Horizontal"
                                    Spacing="8">
                                    <ui:FontIcon
                                        Width="20"
                                        Height="20"
                                        Margin="0,4,0,2"
                                        FontFamily="{StaticResource FluentIconFontFamily}"
                                        FontSize="16"
                                        Foreground="{DynamicResource FloatBarForeground}"
                                        Glyph="&#xe740;" />
                                    <Label
                                        VerticalAlignment="Center"
                                        Content="双指缩放"
                                        FontSize="12"
                                        Foreground="{DynamicResource FloatBarForeground}" />
                                    <Viewbox Width="36" Height="25">
                                        <ui:ToggleSwitch
                                            Name="ToggleSwitchEnableTwoFingerZoom"
                                            Width="42"
                                            MinWidth="0"
                                            IsOn="False"
                                            OffContent=""
                                            OnContent=""
                                            Toggled="ToggleSwitchEnableTwoFingerZoom_Toggled" />
                                    </Viewbox>
                                </ikw:SimpleStackPanel>
                                <ikw:SimpleStackPanel
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Opacity="{Binding ElementName=TwoFingerGestureSimpleStackPanel, Path=Opacity}"
                                    Orientation="Horizontal"
                                    Spacing="8">
                                    <ui:FontIcon
                                        Width="20"
                                        Height="20"
                                        Margin="0,4,0,2"
                                        FontFamily="{StaticResource FluentIconFontFamily}"
                                        FontSize="16"
                                        Foreground="{DynamicResource FloatBarForeground}"
                                        Glyph="&#xe7ad;" />
                                    <Label
                                        VerticalAlignment="Center"
                                        Content="双指旋转"
                                        FontSize="12"
                                        Foreground="{DynamicResource FloatBarForeground}" />
                                    <Viewbox Width="36" Height="25">
                                        <ui:ToggleSwitch
                                            Name="ToggleSwitchEnableTwoFingerRotation"
                                            Width="42"
                                            MinWidth="0"
                                            OffContent=""
                                            OnContent=""
                                            Toggled="ToggleSwitchEnableTwoFingerRotation_Toggled" />
                                    </Viewbox>
                                </ikw:SimpleStackPanel>
                            </ikw:SimpleStackPanel>
                        </ikw:SimpleStackPanel>
                    </Border>
                </Grid>

                <Border
                    x:Name="BtnPPTSlideShowEnd"
                    Width="34"
                    Height="36"
                    Margin="2,0,0,0"
                    Background="{DynamicResource FloatBarBackground}"
                    BorderBrush="{DynamicResource FloatBarBorderBrush}"
                    BorderThickness="1"
                    CornerRadius="4"
                    MouseDown="Border_MouseDown"
                    MouseUp="BtnPPTSlideShowEnd_Click"
                    Visibility="Visible">
                    <Grid>
                        <Image
                            Width="30"
                            Height="20"
                            Source="{DynamicResource ResourceKey=PPTExitNormal}" />
                    </Grid>
                </Border>
            </ikw:SimpleStackPanel>
        </Viewbox>
        <!--  // 笑脸拖动 //  -->
        <Grid
            Name="GridForFloatingBarDraging"
            Background="#01000000"
            MouseMove="SymbolIconEmoji_MouseMove"
            MouseUp="SymbolIconEmoji_MouseUp"
            Visibility="Collapsed" />
        <!--  // 墨迹播放 //  -->
        <InkCanvas
            Name="InkCanvasForInkReplay"
            Background="Transparent"
            EditingMode="None"
            MouseDown="InkCanvasForInkReplay_MouseDown"
            Visibility="Collapsed" />
        <!--  // 侧边栏 //  -->
        <Viewbox
            x:Name="LeftSidePanel"
            Width="30"
            Height="50"
            Margin="-40,0,0,-150"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            MouseUp="UnFoldFloatingBar_MouseUp"
            Visibility="Visible">
            <ikw:SimpleStackPanel>
                <Border
                    Width="30"
                    Height="50"
                    Background="LightGray"
                    CornerRadius="5"
                    Opacity="0.4">
                    <Image
                        Width="30"
                        Height="50"
                        Margin="0,0,-15,0"
                        RenderOptions.BitmapScalingMode="HighQuality"
                        Source="/Resources/Icons-png/penUpright.png" />
                </Border>
            </ikw:SimpleStackPanel>
        </Viewbox>
        <Viewbox
            x:Name="RightSidePanel"
            Width="30"
            Height="50"
            Margin="0,0,-40,-150"
            HorizontalAlignment="Right"
            VerticalAlignment="Center"
            MouseUp="UnFoldFloatingBar_MouseUp"
            Visibility="{Binding ElementName=LeftSidePanel, Path=Visibility}">
            <ikw:SimpleStackPanel>
                <Border
                    Width="30"
                    Height="50"
                    Background="LightGray"
                    CornerRadius="5"
                    Opacity="0.4">
                    <Image
                        Width="30"
                        Height="50"
                        Margin="-15,0,0,0"
                        RenderOptions.BitmapScalingMode="HighQuality"
                        Source="/Resources/Icons-png/penUpright.png" />
                </Border>
            </ikw:SimpleStackPanel>
        </Viewbox>
    </Grid>
</Window>